<MatchingItems>:
    name:'matchingitems'
    AnchorLayout:
        anchor_x: 'center'
        anchor_y: 'top'
        name: 'screen2'
        GridLayout:
            
            cols: 3
            padding: 50
            Button:
                text: "item 1"
                on_release: app.root.current='showitems'
            Button:
                text: "item 2"
            Button:
                text: "item 3"
            Button:
                text: "item 4"
            Button:
                text: "item 5"
            Button:
                text: "item 6"
            Button:
                text: "item 7"
            Button:
                text: "item 8"
            Button:
                text: "item 9"
            Button:
                text: "item 10"
            Button:
                text: "item 11"
            Button:
                text: "item 12"
    AnchorLayout:
        anchor_x: 'center'
        anchor_y: 'bottom'
        BoxLayout:
            orientation: 'horizontal'
            size_hint: 1, .1
            Button:
                text: 'home'
                on_press:app.root.current = 'homepage'
            Button:
                text: 'prev'
                on_press: app.root.current = '#'
            Button:
                text: 'next'
                on_press: app.root.current = '#'